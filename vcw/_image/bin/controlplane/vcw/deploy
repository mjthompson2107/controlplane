#!/usr/bin/env bash
(
  export PATH="$VWC_EXECUTIONPLANE_APIINTERNALPATH_VCWC"
  . executionplane-invoke "$0 $@"
  imageName="${@: -1}"
  . invoke-script $imageName environment

  case $1 in
    --info)
      . info-summary deploy
    ;;
    *)
      executionplane-error "Method not recognised: $1."
    ;;
  esac
  executionplane-complete
)
